{
    "name": "haokeyingxiao/elasticsearch",
    "description": "Elasticsearch for Shopware",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Shopware\\Elasticsearch\\": ""
        },
        "exclude-from-classmap": [
            "/Test/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://issues.haokeyingxiao.com",
        "forum": "https://forum.haokeyingxiao.com",
        "wiki": "https://developer.haokeyingxiao.com",
        "docs": "https://developer.haokeyingxiao.com",
        "chat": "https://slack.haokeyingxiao.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.6.x-dev",
            "dev-trunk": "6.6.x-dev"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~8.2.0 || ~8.3.0",
        "ext-curl": "*",
        "doctrine/dbal": "^3.7",
        "monolog/monolog": "^3.3.1",
        "opensearch-project/opensearch-php": "^2.0",
        "haokeyingxiao/core": "*",
        "shyim/opensearch-php-dsl": "^1.0.5",
        "symfony/http-foundation": "~7.0.8",
        "symfony/messenger": "~7.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
