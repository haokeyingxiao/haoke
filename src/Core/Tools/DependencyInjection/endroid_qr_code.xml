<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Endroid\QrCode\Writer\BinaryWriter" />
        <service id="Endroid\QrCode\Writer\DebugWriter" />
        <service id="Endroid\QrCode\Writer\EpsWriter" />
        <service id="Endroid\QrCode\Writer\GifWriter" />
        <service id="Endroid\QrCode\Writer\PdfWriter" />
        <service id="Endroid\QrCode\Writer\PngWriter" />
        <service id="Endroid\QrCode\Writer\SvgWriter" />
        <service id="Endroid\QrCode\Writer\WebPWriter" />

        <service id="Endroid\QrCode\Builder\Builder" />
        <service id="Endroid\QrCode\Builder\BuilderInterface" alias="Endroid\QrCode\Builder\Builder" />

        <service id="Endroid\QrCode\Builder\BuilderRegistry" />
        <service id="Endroid\QrCode\Builder\BuilderRegistryInterface" alias="Endroid\QrCode\Builder\BuilderRegistry" />

        <service id="Shopware\Core\Tools\Adapter\Twig\Extension\QrCodeExtension">
            <argument type="service" id="Endroid\QrCode\Builder\BuilderRegistryInterface" />
            <tag name="twig.extension" />
        </service>
    </services>

</container>
